{
  "plugins": [
    "@babel/transform-flow-strip-types",
    "@babel/plugin-proposal-class-properties"
  ],
  "presets": [
    [
      "@babel/env",
      {
        "exclude": [
          "transform-regenerator"
        ],
        "loose": true,
        "targets": {
          "browsers": [
            ">1%"
          ]
        },
        "useBuiltIns": false
      }
    ],
    "@babel/preset-react"
  ]
}
